{
  "name": "meetflix-login",
  "version": "0.0.1",
  "description": "test server at port 3000 for oauth login",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-debug": "browserify --node main.js -d -o app.js",
    "build-release": "browserify --node main.js | uglifyjs -c > app.min.js",
    "build": "npm run build-debug && npm run build-release",
    "watch": "watchify main.js -o app.js",
    "dev": "nodemon ./main.js --watch src --watch main.js --exec babel-node --presets es2015"
  },
  "author": "sekwon choi",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "body-parser": "^1.15.0",
    "browserify": "^13.0.0",
    "express": "^4.13.4",
    "mongodb": "^2.1.16",
    "mongoose": "^4.4.10",
    "require": "^2.4.20",
    "simple-oauth2": "^0.5.1",
    "uglify-js": "^2.6.2",
    "util": "^0.10.3",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "nodemon": "^1.9.1",
    "parallelshell": "^2.0.0",
    "webpack": "^1.12.3",
    "webpack-node-externals": "^1.0.0"
  }
}
